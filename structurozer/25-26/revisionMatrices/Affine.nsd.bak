		<for text="&#34;for (int counter = 0; counter &#60; texte.Length; counter++)&#34;" comment="&#34;&#34;" counterVar="dummy_counter" startValue="for (int counter == 0; counter &#60; texte.Length; counter++)" stepConst="1" reliable="false" color="ffffff">
